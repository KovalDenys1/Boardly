{
  "name": "boardly",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:all": "concurrently \"npm run socket:dev\" \"npm run dev\" --names \"socket,next\" --prefix-colors \"blue,magenta\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "socket:dev": "tsx socket-server.ts",
    "socket:start": "tsx socket-server.ts",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:setup": "prisma db push && prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.16.2",
    "@sentry/nextjs": "^10.27.0",
    "@types/bcrypt": "5.0.2",
    "@types/canvas-confetti": "^1.9.0",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.5.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "autoprefixer": "10.4.20",
    "bcrypt": "5.1.1",
    "canvas-confetti": "^1.9.3",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "9.0.2",
    "nanoid": "^5.1.6",
    "next": "^14.2.11",
    "next-auth": "^4.24.7",
    "postcss": "8.4.47",
    "prisma": "5.16.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "resend": "^6.1.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "3.4.14",
    "tsx": "^4.20.6",
    "typescript": "5.6.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
